{
  "name": "heatwallet",
  "version": "1.0.1",
  "description": "Heat Ledger Generic Client Framework",
  "author": "Heat Ledger Ltd.",
  "homepage": "http://heatledger.com",
  "license": "MIT",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heat-Ledger-Ltd/heat-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Heat-Ledger-Ltd/heat-ui/issues"
  },
  "scripts": {
    "pack": "build --dir",
    "dist": "build"
  },
  "devDependencies": {
    "connect": "3.4.1",
    "del": "2.2.2",
    "electron-builder": "11.7.0",
    "electron-builder-squirrel-windows": "^11.6.1",
    "electron-packager": "^8.5.1",
    "electron-prebuilt": "1.3.3",
    "es6-promise": "3.2.1",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.0",
    "gulp-minify-css": "1.2.4",
    "gulp-minify-html": "1.0.6",
    "gulp-replace": "0.5.4",
    "gulp-rev": "7.1.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "2.13.6",
    "gulp-uglify": "2.0.0",
    "gulp-usemin": "0.3.24",
    "open": "0.0.5",
    "serve-static": "1.11.1"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.0",
    "angular-material-data-table": "0.10.9",
    "angular-messages": "1.5.8",
    "angular-new-router": "0.5.3",
    "angular-route": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-scroll": "^1.0.0",
    "big.js": "^3.1.3",
    "bytebuffer": "5.0.1",
    "child-process-promise": "^2.2.0",
    "dateformat": "1.0.12",
    "filesaver.js": "0.2.0",
    "long": "3.2.0",
    "material-design-icons": "2.2.3",
    "pako": "1.0.3",
    "techan": "git://github.com/andredumas/techan.js",
    "zeroclipboard": "2.2.0"
  },
  "build": {
    "appId": "com.heatledger.heatwallet",
    "copyright": "Copyright Â© 2016 Heat Ledger Ltd.",
    "productName": "Heatwallet",
    "compression": "normal",
    "extraResources": [{
      "from": "../heatledger/build/install/heatledger",
      "to": "heatledger"
    }],
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/Heat-Ledger-Ltd/heat-ui/e03bf2ba200afacf37007bf7569dc51011e67815/build/icon.ico",
      "target": [
        "squirrel"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "category": "public.app-category.finance",
      "target": [
        "zip"
      ]
    },
    "directories": {
      "output": "releases",
      "app": "dist"
    }
  }
}
